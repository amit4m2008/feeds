= form_for(@feed, html: {class: "form-horizontal"}) do |f|
  - if @feed.errors.any?
    #error_explanation
      %h2
        = pluralize(@feed.errors.count, "error")
        prohibited this feed from being saved:
      %ul
        - @feed.errors.full_messages.each do |message|
          %li= message

  .form-group
    = f.label :feed_url
    = f.text_field :feed_url, class: 'form-control'
  .form-group
    = f.submit "Save", class: 'btn btn-success'
    = link_to 'Back', feeds_path, class: 'btn btn-primary'